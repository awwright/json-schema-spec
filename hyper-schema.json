{
    "$schema": "http://json-schema.org/draft-08/hyper-schema#",
    "$id": "http://json-schema.org/draft-08/hyper-schema#",
    "title": "JSON Hyper-Schema",
    "$recursiveRef": "http://json-schema.org/draft-08/schema#",
    "properties": {
        "links": {
            "type": "array",
            "items": {
                "$recursiveRef": "http://json-schema.org/draft-08/links-recursive#"
            }
        }
    },
    "links": [
        {
            "rel": "self",
            "href": "{+%24id}"
        }
    ]
}
